{
  "reporter": [
    "lcov",
    "text-summary"
  ],
  "extension": [
    ".ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "dist",
    "test/**/*.spec.ts",
    "src/index.ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "sourceMap": true,
  "instrument": true,
  "check-coverage": true,
  "cache": true,
  "branches": 60,
  "lines": 80,
  "functions": 75,
  "statements": 80
}
