{
  "name": "@hapify/gui",
  "version": "1.2.0",
  "description": "Graphical User Interface for Hapify",
  "keywords": [
    "hapify",
    "cli"
  ],
  "license": "MIT",
  "author": "Edouard Demotes-Mainard <edouard@tractr.net>",
  "homepage": "https://github.com/hapify/hapify",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod --configuration=production",
    "start": "ng serve",
    "clean": "rimraf dist node_modules package-lock.json src/assets/ace-builds/src-min-noconflict",
    "prepublishOnly": "npm run build",
    "lint": "npm run lint:tsc && npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint \"{src,test}/**/*.{ts,js}\"",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:fix": "npm run lint:tsc:fix && npm run lint:prettier:fix && npm run lint:eslint:fix",
    "lint:prettier": "prettier --check \"{src,test}/**/*.{ts,js}\"",
    "lint:prettier:fix": "prettier --write \"{src,test}/**/*.{ts,js}\"",
    "lint:tsc": "tsc --noEmit",
    "lint:tsc:fix": "npm run lint:tsc",
    "test": "echo 'No tests for @hapify/gui'",
    "e2e": "ng e2e",
    "update": "npm-check -u"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/animations": "~10.0.6",
    "@angular/cdk": "^10.1.2",
    "@angular/cli": "~10.0.5",
    "@angular/common": "~10.0.6",
    "@angular/compiler": "~10.0.6",
    "@angular/compiler-cli": "~10.0.6",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~10.0.6",
    "@angular/material": "^10.1.2",
    "@angular/platform-browser": "~10.0.6",
    "@angular/platform-browser-dynamic": "~10.0.6",
    "@angular/router": "~10.0.6",
    "@ng-bootstrap/ng-bootstrap": "^7.0.0",
    "@ng-bootstrap/schematics": "^2.0.0-alpha.1",
    "@ngx-meta/core": "^9.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/md5": "^2.2.0",
    "angular2-hotkeys": "^2.2.0",
    "bootstrap": "^4.5.0",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "md5": "^2.3.0",
    "ng2-ace-editor": "^0.3.9",
    "ng4-click-outside": "^1.0.1",
    "ngx-bootstrap": "^5.6.1",
    "ngx-perfect-scrollbar": "^10.0.1",
    "nl2br-pipe": "^1.2.3",
    "open-iconic": "^1.1.1",
    "protractor": "~7.0.0",
    "rxjs": "^6.6.2",
    "tslib": "^2.0.0",
    "typescript": "~3.9.5",
    "zone.js": "~0.10.3"
  }
}
